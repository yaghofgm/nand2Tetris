/**
 * Exclusive-or gate:
 * if ((a and Not(b)) or (Not(a) and b)) out = 1, else out = 0
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=notA);
    Not(in=b, out=notB);
    And(a=a,b=notB,out=AnotB);
    And(a=notA,b=b,out=notAB);
    Or(a=AnotB,b=notAB,out=out);
}